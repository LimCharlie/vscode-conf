{
  // open json editor for settings
  "workbench.settings.editor": "json",
  // Change font
  "editor.fontFamily": "Geist Mono",
  "scm.inputFontFamily": "DaddyTimeMono NF",
  "chat.editor.fontFamily": "DaddyTimeMono NF",
  "debug.console.fontFamily": "DaddyTimeMono NF",
  "editor.codeLensFontFamily": "DaddyTimeMono NF",
  "notebook.output.fontFamily": "DaddyTimeMono NF",
  "markdown.preview.fontFamily": "DaddyTimeMono NF",
  "editor.inlayHints.fontFamily": "DaddyTimeMono NF",
  // Font size
  "editor.fontSize": 14,
  // terminal
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "DaddyTimeMono NF",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  // ui
  "editor.minimap.enabled": false,
  "window.commandCenter": false,
  "editor.scrollbar.vertical": "auto",
  "window.titleBarStyle": "custom",
  "editor.scrollbar.horizontal": "auto",
  // APC
  "apc.activityBar": {
    "position": "bottom",
    "hideSettings": true,
    "size": 28
  },
  "apc.electron": {
    "titleBarStyle": "hiddenInset",
    "trafficLightPosition": {
      "x": 12,
      "y": 10
    }
  },
  "apc.font.family": "Geist Mono",
  "apc.monospace.font.family": "Geist Mono",
  "apc.statusBar": {
    "position": "editor-bottom",
    "height": 28,
    "fontSize": 12
  },
  // Cusom css
  "apc.stylesheet": {
    ".sidebar > .composite": "height: 12px !important",
    ".title-actions": "display: none !important",
    ".tabs-and-actions-container > .editor-actions": "display: none !important",
    ".titlebar-center": "display: none !important",
    ".titlebar-right": "display: none !important",
    ".inline-tabs-placeholder": "display: none !important",
    ".split-view-view .visible": "background-color: #16112f !important"
  },
  // Editor
  "editor.tabSize": 2,
  "breadcrumbs.enabled": false,
  "editor.renderWhitespace": "trailing",
  "editor.fontWeight": "500",
  "editor.guides.indentation": true,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "never",
  "editor.snippetSuggestions": "top",
  "editor.suggest.insertMode": "replace",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.occurrencesHighlight": "off",
  "editor.cursorBlinking": "expand",
  "editor.linkedEditing": true,
  "editor.insertSpaces": true,
  "editor.cursorStyle": "line-thin",
  "editor.detectIndentation": false,
  "editor.fontLigatures": "true",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": true,
  "editor.accessibilitySupport": "off",
  "editor.suggest.preview": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.tabCompletion": "off",
  "editor.suggestSelection": "first",
  "editor.wordBasedSuggestions": "matchingDocuments",
  "editor.parameterHints.enabled": true,
  "editor.quickSuggestionsDelay": 10,
  "editor.formatOnSave": false,
  "security.workspace.trust.untrustedFiles": "open",
  // EXPLORER
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "explorer.decorations.badges": true,
  "explorer.autoReveal": true,
  // FILES
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/log": true,
    "**/node_modules": true
  },
  "files.associations": {
    "*.erb": "erb",
    "Gemfile": "ruby",
    "*.html.erb": "erb"
  },
  "[erb]": {
    "editor.defaultFormatter": "aliariff.vscode-erb-beautify"
  },
  "search.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/log": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/tmp": true,
    "**/coverage": true
  },
  "search.useGlobalIgnoreFiles": true,
  "search.useIgnoreFiles": true,
  // Workbench
  "workbench.iconTheme": "moxer-icons",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 15,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 6,
  "workbench.colorTheme": "Bearded Theme Vivid Purple",
  "workbench.colorCustomizations": {
    "[Bearded Theme Vivid Purple]": {
      "editor.background": "#16112f",
      "terminal.background": "#16112f",
      "activityBar.background": "#16112f",
      "statusBar.background": "#16112f",
      "editorGroupHeader.tabsBackground": "#16112f",
      "tab.inactiveBackground": "#16112f"
    },
    "editorIndentGuide.activeBackground1": "#E06c75",
    "tab.activeBackground": "#282c34",
    "activityBar.background": "#282c34",
    "tree.indentGuidesStroke": "#05ef3c",
    "tab.activeBorder": "#d19a66",
  },
  // JS
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.suggestSelection": "recentlyUsed",
    "editor.suggest.showKeywords": false
  },
  // EMMET
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "erb": "html",
    "javascript": "javascriptreact"
  },
  //GIT
  "git.ignoreMissingGitWarning": true,
  "git.mergeEditor": false,
  "git.autofetch": true,
  //HTML
  "html.hover.documentation": false,
  "html.completion.attributeDefaultValue": "singlequotes",
  //RUBY
  "ruby.rubocop.executePath": "/Users/lim_c/.rvm/gems/ruby-3.3.0/bin/",
  "extensions.ignoreRecommendations": true,
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop",
    "editor.semanticHighlighting.enabled": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
  },
  "solargraph.completion": true,
  "solargraph.definitions": true,
  "scss.format.spaceAroundSelectorSeparator": true,
  "css.format.spaceAroundSelectorSeparator": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "ruby.rubocop.suppressRubocopWarnings": true,
  "ruby.rubocop.useBundler": true,
  "vscode-erb-beautify.keepBlankLines": 1,
  "vsicons.dontShowNewVersionMessage": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.currentLine.enabled": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "multiCommand.commands": [
    {
      "command": "multiCommand.MakeRoom",
      "sequence": [
        "workbench.action.toggleSidebarVisibility",
      ]
    }
  ],
  "workbench.editor.enablePreview": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "console-ninja.featureSet": "Community",
  "monokaiPro.fileIconsMonochrome": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.openRepositoryInParentFolders": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  //"workbench.editor.showTabs": "single",
  "zenMode.showTabs": "single",
  "window.zoomLevel": 1,
  "accessibility.signals.lineHasError": {
    "sound": "off"
  },
  "accessibility.signals.lineHasFoldedArea": {
    "sound": "off"
  },
  "accessibility.signals.lineHasBreakpoint": {
    "sound": "off"
  },
  "accessibility.signals.lineHasInlineSuggestion": {
    "sound": "off"
  },
  "accessibility.signals.onDebugBreak": {
    "sound": "off"
  },
}

